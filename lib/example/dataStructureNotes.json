{
    "dialect": "mysql",   //使用的数据库，这个例子用mysql
    "host": "192.168.1.131",   //数据库主机地址
    "port": 3306,    //数据库主机端口号
    "user": "admin",   //数据库登录名
    "password": "admin",   //数据库登录密码
    "name": "exampleProject",   //数据库中新建的schema，这个例子新建的名字叫exampleProject
    "tables": [   //需要新建的数据表，数组形式
        {   //其中一个数据表
            "name": "users",   //数据表名称
            "remark": "用户信息表",   //数据表备注
            "group":"account",   //数据表所属分组，这个例子中这个分组只有一个数据表
            "groupRemark":"账号信息组",   //分组备注
            "fields": [   //数据表字段
                {
                    "name": "id",   //字段名称
                    "remark": "用户信息id",   //字段备注
                    "type": "BIGINT",   //字段类型，这里可填BIGINT、TINYINT、VARCHAR
                    "length": 8,   //类型长度
                    "defaultValue": 0,   //默认值
                    "primaryKey": true   //是否主键
                },
                {
                    "name": "name",
                    "remark": "用户名称",
                    "type": "VARCHAR",
                    "length": 30,
                    "defaultValue": "''",
                    "unique": true   //是否唯一
                },
                {
                    "name": "sex",
                    "remark": "用户性别",
                    "type": "VARCHAR",
                    "length": 10,
                    "defaultValue": "男"
                },
                {
                    "name": "age",
                    "remark": "用户年龄",
                    "type": "TINYINT",
                    "length": 1,
                    "defaultValue": 0
                }
            ]
        }
    ]
}